<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintCircleRadius="20dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:padding="16dp">

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/download"
            app:shapeAppearanceOverlay="@style/Circular"></com.google.android.material.imageview.ShapeableImageView>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="200dp">

        <TextView
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:background="@drawable/textview_edittext_design"
            android:paddingTop="5dp"
            android:text="KHACHA Soufiane"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textStyle="bold|italic"
            android:id="@+id/completename"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:background="#E5E5E5">


        <TextView
            android:id="@+id/phonenumber_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:drawableStart="@drawable/ic_baseline_phone_24"
            android:drawablePadding="12dp"
            android:padding="5dp"
            android:text="212623995158"
            android:textColor="@color/teal_200"
            android:drawableLeft="@drawable/ic_baseline_phone_24" />

        <TextView
            android:id="@+id/email_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="35dp"
            android:drawableStart="@drawable/ic_baseline_email_24"
            android:drawablePadding="12dp"
            android:padding="5dp"
            android:text="khachasoufiane@gmail.com"
            android:textColor="@color/teal_200"
            android:drawableLeft="@drawable/ic_baseline_email_24" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="340dp"
        >

        <TextView
            android:id="@+id/textView7"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="Date Fin"
            android:layout_marginLeft="220dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:background="@drawable/mission"
            android:padding="4dp"
            android:textColor="@color/white"/>
        <TextView
            android:id="@+id/textView8"
            android:layout_width="90dp"
            android:padding="4dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Date Depart"
            android:layout_marginLeft="50dp"
            android:background="@drawable/mission"
            android:textColor="@color/white"
            />
        <TextView
            android:id="@+id/startdate_profile"
            android:layout_width="90dp"
            android:padding="4dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="20/05/2021"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="50dp"/>

        <TextView
            android:id="@+id/finishdate_profile"
            android:layout_width="90dp"
            android:padding="4dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="21/05/2021"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="220dp"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="410dp"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/mission_layout">


        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:textAlignment="center"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="Mission"
            android:textSize="18dp"
            android:textColor="@color/white"
            android:textStyle="bold|italic"
            android:background="@drawable/mission"/>

        <TextView
            android:id="@+id/startplace_profile"
            android:layout_width="90dp"
            android:padding="4dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Al Hoceima"
            android:layout_marginTop="95dp"
            android:layout_marginLeft="20dp"/>

        <TextView
            android:id="@+id/finishplace_profile"
            android:layout_width="90dp"
            android:padding="4dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Tanger"
            android:layout_marginTop="95dp"
            android:layout_marginLeft="150dp"/>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="90dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Lieu Depart"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/mission"
            android:padding="4dp"
            android:textColor="@color/white"/>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="90dp"
            android:padding="4dp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:layout_height="wrap_content"
            android:text="Lieu Arriver"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="150dp"
            android:background="@drawable/mission"
            android:textColor="@color/white"/>
    </RelativeLayout>







</RelativeLayout>